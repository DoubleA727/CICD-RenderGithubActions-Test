{
  "dependencies": {
    "@huggingface/inference": "^4.13.4",
    "@prisma/adapter-pg": "^5.20.0",
    "@prisma/client": "^6.18.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^7.4.2",
    "ejs": "^3.1.10",
    "express": "^4.22.1",
    "googleapis": "^167.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.11",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.0",
    "pupeteer": "^0.0.1",
    "puppeteer": "^24.32.1",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.3",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^22.7.2",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "prisma": "^6.18.0"
  },
  "scripts": {
    "postinstall": "npx prisma generate",
    "migration:dev": "dotenv -e .env.production -- npx prisma migrate dev",
    "migration:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "migration:reset": "dotenv -e .env.test -- npx prisma migrate reset",
    "production": "dotenv -e /etc/secrets/_env_production -- node ./src/server.js",
    "start": "dotenv -e .env.test -- nodemon ./src/server.js",
    "test": "dotenv -e .env.test -- playwright test --config=configs/playwright.config.js",
    "lint": "eslint -c configs/eslint.config.mjs ./src/*",
    "test-trace": "npm test -- --reporter=html --trace on",
    "dev": "nodemon index.js",
    "init": "dotenv -e .env.test -- node ./prisma/data/initUsers && dotenv -e .env.test -- node ./prisma/seed.js",
    "nuke": "dotenv -e .env.test -- node ./prisma/truncateAll.js",

    "start:testserver": "dotenv -e .env.test -- node ./src/server.js",
    "test:login": "dotenv -e .env.test -- playwright test tests/login.spec.js --config=configs/playwright.config.js"
  }
}
